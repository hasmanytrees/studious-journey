        <fullName>Escalation_Reason__c</fullName>
        <description>This field is used to capture the escalation reason for the case. It should be mandatory for the end user to fill in this field whenever case is Escalated</description>
        <externalId>false</externalId>
        <inlineHelpText>Select the reason for Escalation (for use when Escalating a Case)</inlineHelpText>
        <label>Escalation Reason</label>
        <picklist>
            <controllingField>Status</controllingField>
            <picklistValues>
                <fullName>Information Unavailable</fullName>
                <controllingFieldValues>Escalated</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>No Response - A response has not been given, so an escalation is appropriate</fullName>
                <controllingFieldValues>Escalated</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Partial Response - The response received did not fully resolve the I3 and follow up is needed by the owner of the I3 or super user</fullName>
                <controllingFieldValues>Escalated</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Unsatisfactory Response - The response received was unsatisfactory AND communication with the owner to resolve was ineffective</fullName>
                <controllingFieldValues>Escalated</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Lack of Resources - Missing a needed resource in order to resolve this I3, ie. a PF isnâ€™t answering my emails, or returning  a report, etc. that is outside of my control</fullName>
                <controllingFieldValues>Escalated</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Workload Concern - Do not have the time to solve by the due date due to my workload</fullName>
                <controllingFieldValues>Escalated</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Assistance Needed or Knowledge Gap - Need a super user to look at this request with me and help me figure out what needs to be done</fullName>
                <controllingFieldValues>Escalated</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Information Unavailable - Unable to attain the needed information from the project, etc., and need help in getting the correct information</fullName>
                <controllingFieldValues>Escalated</controllingFieldValues>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Expected_Resolution_Date__c</fullName>
        <description>Expected Date of Resolution</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Status, &apos;Closed&apos;), Case_Urgency_Temp__c,  IF( ISPICKVAL(Severity__c, &apos;High&apos;)   , CreatedDate + $Setup.Case_Urgency_Configuration__c.Urgency_High__c,IF( ISPICKVAL(Severity__c, &apos;Medium&apos;), CreatedDate + $Setup.Case_Urgency_Configuration__c.Urgency_Medium__c,IF( ISPICKVAL(Severity__c, &apos;Low&apos;), CreatedDate + $Setup.Case_Urgency_Configuration__c.Urgency_Low__c, Now())) ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This is a System-calculated field that shows the date by which this case is expected to be resolved (based on the Priority)</inlineHelpText>
        <label>Expected Resolution Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
