    <fields>
        <fullName>Priority__c</fullName>
        <description>Depicts priority of the case. 
Priority is automatically calculated based on the urgency and impact values at the time of case creation.</description>
        <externalId>false</externalId>
        <formula>IF(
	AND(
		ISPICKVAL(Impact__c, &apos;High&apos;), 
		ISPICKVAL(Severity__c, &apos;High&apos;)
	), 
	&apos;Critical&apos;,
	IF(
		OR(
			AND(
				ISPICKVAL(Impact__c, &apos;Medium&apos;), 
				ISPICKVAL(Severity__c, &apos;High&apos;)
			),
			AND(
				ISPICKVAL(Impact__c, &apos;High&apos;), 
				ISPICKVAL(Severity__c, &apos;Medium&apos;)
			),
			AND(
				ISPICKVAL(Impact__c, &apos;Low&apos;), 
				ISPICKVAL(Severity__c, &apos;High&apos;)
			)
		),
		&apos;High&apos;,
		IF(
			OR(
				AND(
					ISPICKVAL(Impact__c, &apos;Medium&apos;), 
					ISPICKVAL(Severity__c, &apos;Medium&apos;)
				),
				AND(
					ISPICKVAL(Impact__c, &apos;High&apos;), 
					ISPICKVAL(Severity__c, &apos;Low&apos;)
				)
			),
			&apos;Medium&apos;,
			IF(
				OR(
					AND(
						ISPICKVAL(Impact__c, &apos;Low&apos;), 
						ISPICKVAL(Severity__c, &apos;Medium&apos;)
					),
					AND(
						ISPICKVAL(Impact__c, &apos;Medium&apos;), 
						ISPICKVAL(Severity__c, &apos;Low&apos;)
					),
					AND(
						ISPICKVAL(Impact__c, &apos;Low&apos;), 
						ISPICKVAL(Severity__c, &apos;Low&apos;)
					)
				),
				&apos;Low&apos;,
				&apos;&apos;
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
