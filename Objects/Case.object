    <webLinks>
        <fullName>Claim_Case</fullName>
        <availability>online</availability>
        <description>This button flips the Status of the Case to &quot;Claimed&quot;</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Claim Case</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

var c = new sforce.SObject(&quot;Case&quot;); 
c.id = &quot;{!Case.Id}&quot;;
c.Status = &quot;{!Case.Status}&quot;;
if( c.Status == &apos;Closed&apos;)
{
  var reply = confirm(&apos;You are about to claim this case. Are you sure?&apos;);
  if(reply == true)
  {
    c.Status = &apos;Claimed&apos;; 
    sforce.connection.update([c]); 
    window.location.reload(); 
  }
  else
  window.location.reload();
}
else
{
  alert(&apos;You can only claim a &quot;Closed&quot; case&apos;);
  window.location.reload();
}</url>
