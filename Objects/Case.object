    <validationRules>
        <fullName>Case_Creator_can_not_change_the_priority</fullName>
        <active>false</active>
        <description>This is to ensure that the case creator cannot edit the case priroty</description>
        <errorConditionFormula>($User.Id= CreatedBy.Id)  &amp;&amp; ISCHANGED(Priority)</errorConditionFormula>
        <errorMessage>Case Initiator does not have the capability to change the case priority</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Case_Status</fullName>
        <active>true</active>
        <description>A new case can only be created in new status</description>
        <errorConditionFormula>IF(ISNEW(), NOT(ISPICKVAL(Status, &apos;NEW&apos;)), FALSE)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Case can only be created in &quot;New&quot; status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Case_owner_can_change_the_priority</fullName>
        <active>false</active>
        <description>This is to ensure that the case priority is changed by case owner only and no one else</description>
        <errorConditionFormula>(OwnerId != $User.Id) &amp;&amp; ISCHANGED(Priority)</errorConditionFormula>
        <errorDisplayField>Priority</errorDisplayField>
        <errorMessage>Only Case Owner is allowed to change the priority.</errorMessage>
    </validationRules>
