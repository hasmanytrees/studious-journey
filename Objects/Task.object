    <validationRules>
        <fullName>Milestone_completion_date_change</fullName>
        <active>false</active>
        <description>Milestone completion date can be changed only within six months of ICP agreement date.</description>
        <errorConditionFormula>AND(
     ISCHANGED(  ActivityDate ),
      ActivityDate -ICP_Agreement_Date__c &gt; 180
    )</errorConditionFormula>
        <errorDisplayField>ActivityDate</errorDisplayField>
        <errorMessage>Due Date must be less than 6 months after the Partnership Agreement Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_ICP_Users_From_Editing_Task</fullName>
        <active>true</active>
        <description>This validation rule will prevent ICP users from Editing tasks that are not assigned to them.</description>
        <errorConditionFormula>AND(NOT(ISNEW()), ISPICKVAL($User.UserType, &apos;PowerPartner&apos;),OwnerId !=  $User.Id, CreatedById != $User.Id )</errorConditionFormula>
        <errorMessage>You are only able to edit tasks that are assigned to you.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Task_Assignation_For_ICP_Users</fullName>
        <active>true</active>
        <description>This Validation rule will prevent ICP users to assign task to any other users not belonging to their ICP.</description>
        <errorConditionFormula>AND( OR(ISNEW(), CreatedById =  $User.Id),ISPICKVAL($User.UserType, &apos;PowerPartner&apos;),NOT( ISPICKVAL(Owner:User.UserType, &apos;PowerPartner&apos;) ))</errorConditionFormula>
        <errorMessage>You are only able to assign tasks to users within your ICP. Please contact your Partnership Facilitator to discuss this task with them.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WhenStatusComppleted_ActivityCompletionD</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK( Activity_Completion_Date__c ) , ISPICKVAL(Status, &apos;Completed&apos;) )</errorConditionFormula>
        <errorDisplayField>Activity_Completion_Date__c</errorDisplayField>
        <errorMessage>Please Enter the Activity Completion Date.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Delete_Activities</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delete Activities</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)} 


var recordTaskList={!GETRECORDIDS( $ObjectType.Task )}; 
var recordEventList={!GETRECORDIDS(  $ObjectType.Event  )}; 
if(recordTaskList.length==0)
{ 
 if(recordEventList.length==0)
{
alert(&quot;Please select at least a single record !&quot;); 
}

} 
else 
{ 
var choice=confirm(&quot;Do you want to delete selected Record(s) ?&quot;); 
if(choice==true) 
{ 
if(recordTaskList.length!=0)
{
result = sforce.connection.deleteIds(recordTaskList); 
alert(recordTaskList.length+&quot; Task(s) Deleted Successfully &quot;); 
}

if(recordEventList.length!=0)
{
result = sforce.connection.deleteIds(recordEventList);
alert(recordEventList.length+&quot; Event(s) Deleted Successfully &quot;);  
}
//alert(recordList.length+&quot; Task(s) Deleted Successfully &quot;); 
location.reload(); 
} 
}</url>
