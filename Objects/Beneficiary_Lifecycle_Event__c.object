        <errorConditionFormula>AND( Not(ISPICKVAL($User.UserType, &apos;PowerPartner&apos;)) , ISCHANGED( Review_Status__c ),ISPICKVAL( Review_Status__c , &apos;Submitted&apos;), Beneficiary__r.RecordType.DeveloperName=&apos;CDSP_Beneficiary&apos; )</errorConditionFormula>
