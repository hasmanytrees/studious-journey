    <validationRules>
        <fullName>Death_Category_Mandatory</fullName>
        <active>true</active>
        <description>If Beneficiary Death is selected as the Unplanned Exit Reason, Death Category and Sub category becomes mandatory</description>
        <errorConditionFormula>!($Label.Disable_For_Data_Load==&apos;true&apos;) &amp;&amp; RecordType.DeveloperName = &apos;Unplanned_Exit&apos; &amp;&amp;  ISPICKVAL(Reason_For_Request__c,&apos;Death of child&apos;) &amp;&amp; (ISBLANK(TEXT(Death_Category__c)) || (ISBLANK(TEXT(Death_Sub_Category__c)) &amp;&amp;  !ISPICKVAL(Death_Category__c, &apos;Unknown Cause&apos;) ) ||  ISBLANK( Date_Of_Death__c ) )</errorConditionFormula>
        <errorMessage>Please Enter Date Of Death, Death Category and Death Sub-Category</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Death_Date_cant_be_future_date</fullName>
        <active>true</active>
        <description>This validation rule restricts User to give Date of Death as future date.</description>
        <errorConditionFormula>(RecordType.DeveloperName  = &apos;Unplanned_Exit&apos; || RecordType.DeveloperName  = &apos;Caregiver_Death&apos;)&amp;&amp; OR( Date_Of_Death__c &lt; Beneficiary__r.PersonBirthdate , Date_Of_Death__c &gt; TODAY())</errorConditionFormula>
        <errorMessage>Date of Death can&apos;t be future date (or) Date of Death can&apos;t be less than Birthdate</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Death_Date_greater_than_30_or_90_days</fullName>
        <active>false</active>
        <description>If Death Date is greater than 30 days for CDSP Center Based and more than 90 days for CSP &amp; CDSP Home Based then user must enter the delay reason in Reason_For_Delay_In_Processing field.</description>
        <errorConditionFormula>IF(RecordType.DeveloperName=&apos;Unplanned_Exit&apos;,IF(Beneficiary__r.Program_Stage__c=&apos;CDSP Center-Based&apos;,IF(Date_of_Action__c - 30 &gt;  Date_Of_Death__c &amp;&amp; ISBLANK( Reason_For_Delay_In_Processing__c ),true,false),IF(Date_of_Action__c - 90 &gt; Date_Of_Death__c &amp;&amp; ISBLANK( Reason_For_Delay_In_Processing__c ),true,false)),false)</errorConditionFormula>
        <errorDisplayField>Reason_For_Delay_In_Processing__c</errorDisplayField>
        <errorMessage>Enter the reason for delay in processing as amount of time between Date of Death and Effective Date exceeds the specified limit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Death_Dt_cant_be_before_Last_Attnded_Dt</fullName>
        <active>true</active>
        <description>This validation rule restricts Date of Death to be earlier than Last Attended Date</description>
        <errorConditionFormula>(RecordType.DeveloperName=&apos;Unplanned_Exit&apos;) &amp;&amp; (Date_Of_Death__c &lt; Last_Attended_Date__c)</errorConditionFormula>
        <errorMessage>Date of Death can&apos;t be earlier than Last Attended Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Effective_Date_cannot_be_past_date</fullName>
        <active>true</active>
        <description>This validation rule restricts User to give Effective Date as past date for planned exit.</description>
        <errorConditionFormula>($Profile.Name != &apos;System Administrator&apos; )&amp;&amp;RecordType.DeveloperName = &apos;Planned_Exit&apos; &amp;&amp; Date_of_Action__c &lt; TODAY() &amp;&amp; OR(Beneficiary__r.RecordType.DeveloperName=&apos;CSP_Beneficiary&apos;,NOT(NOT(ISPICKVAL($User.UserType, &apos;PowerPartner&apos;)) &amp;&amp; OR(ISPICKVAL(Review_Status__c, &apos;Approved&apos;),ISPICKVAL(Review_Status__c, &apos;Returned To ICP&apos;)) &amp;&amp; ISPICKVAL( PRIORVALUE(Review_Status__c), &apos;Submitted&apos;)))</errorConditionFormula>
        <errorMessage>Effective Date can&apos;t be past date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Effective_Date_grtr_than_30_or_90_days</fullName>
        <active>false</active>
        <description>If Effective Date is greater than Last Attended Date by 30 days for CDSP Center Based and by more than 90 days for CSP &amp; CDSP Home Based then user must enter the delay reason in Reason_For_Delay_In_Processing field.</description>
        <errorConditionFormula>IF(RecordType.DeveloperName=&apos;Planned_Exit&apos; || RecordType.DeveloperName=&apos;Unplanned_Exit&apos;,IF(Beneficiary__r.Program_Stage__c=&apos;CDSP Center-Based&apos;,IF(Date_of_Action__c - 30 &gt; Last_Attended_Date__c &amp;&amp; ISBLANK( Reason_For_Delay_In_Processing__c ),true,false),IF(Date_of_Action__c - 90 &gt; Last_Attended_Date__c &amp;&amp; ISBLANK( Reason_For_Delay_In_Processing__c ),true,false)),false)</errorConditionFormula>
        <errorDisplayField>Reason_For_Delay_In_Processing__c</errorDisplayField>
        <errorMessage>Enter the reason for delay in processing as amount of time between Last Attended Date and Effective Date exceeds the specified limit.</errorMessage>
    </validationRules>
    <validationRules>
