    <validationRules>
        <fullName>Beneficiary_Valid_Age_For_Transition</fullName>
        <active>true</active>
        <description>Checks if Beneficiary&apos;s age is greater than 1 for &apos;Survival to Sponsorship-Home&apos; transition and greater than 3 for &apos;Sponsorship-Home to Sponsorship-Center&apos; at the time of Transition as per Effective Date</description>
        <errorConditionFormula>IF(
			AND(!($Label.Disable_For_Data_Load==&apos;true&apos;),RecordType.DeveloperName = &apos;Transition&apos;), 
			IF(
				ISPICKVAL(Transition_Type__c , &apos;Survival to Sponsorship-Home&apos;),
				OR(Beneficiary__r.Age_In_Months__c &lt;12,ISNULL(Beneficiary__r.Age_In_Months__c)), 
				IF(
					ISPICKVAL(Transition_Type__c ,&apos;Sponsorship-Home to Sponsorship-Center&apos;),
					OR(Beneficiary__r.Age_In_Months__c &lt;36,ISNULL(Beneficiary__r.Age_In_Months__c)),
					FALSE
				) 
			),
			FALSE
		)</errorConditionFormula>
        <errorMessage>Beneficiary&apos;s age should be greater than 1 for &apos;Survival to Sponsorship-Home&apos; transition and greater than 3 for &apos;Sponsorship-Home to Sponsorship-Center&apos; at the time of Transition calculated as per Effective Date field.</errorMessage>
    </validationRules>
    <validationRules>
