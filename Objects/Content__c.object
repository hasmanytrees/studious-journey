        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)} 

var result = sforce.connection.query(&quot;SELECT id,DeveloperName FROM RecordType where sobjecttype=&apos;Content__c&apos;&quot;); 

records = result.getArray(&quot;records&quot;); 

var activityPlan =&apos;&apos;; 
var Beneficiary =&apos;&apos;; 
var Beneficiary_Assessment =&apos;&apos;; 
var Beneficiary_Lifecycle_Event =&apos;&apos;; 
var Field_Office =&apos;&apos;; 
var ICP =&apos;&apos;; 
var ICP_Lead =&apos;&apos;; 
var Disaster =&apos;&apos;; 
for (var i=0; i&lt; records.length; i++) 
{ 
if(records[i].DeveloperName ==&apos;Activity_Plan&apos;){ 
activityPlan = records[i].Id; 

} 
if(records[i].DeveloperName ==&apos;Beneficiary&apos;){ 
Beneficiary = records[i].Id; 

} 

if(records[i].DeveloperName ==&apos;Beneficiary_Assessment&apos;){ 
Beneficiary_Assessment = records[i].Id; 

} 
if(records[i].DeveloperName ==&apos;Beneficiary_Lifecycle_Event&apos;){ 
Beneficiary_Lifecycle_Event = records[i].Id; 

} 
if(records[i].DeveloperName ==&apos;Field_Office&apos;){ 
Field_Office= records[i].Id; 
} 
if(records[i].DeveloperName ==&apos;ICP&apos;){ 
ICP= records[i].Id; 

} 
if(records[i].DeveloperName ==&apos;ICP_Lead&apos;){ 
ICP_Lead= records[i].Id; 

} 
if(records[i].DeveloperName ==&apos;Disaster&apos;){ 
Disaster= records[i].Id; 

} 
} 
if( &apos;{!Disaster__c.Id}&apos; != &apos;&apos;) { 
window.open(&quot;{!$Setup.Org_Level_Settings__c.Content_Object_Prefix__c}/e?CF{!$Setup.Org_Level_Settings__c.Disaster_Content_Id__c}={!JSENCODE(Disaster__c.Name)}&amp;CF{!$Setup.Org_Level_Settings__c.Disaster_Content_Id__c}_lkid={!Disaster__c.Id}&amp;retURL=%2F{!Disaster__c.Id}&amp;RecordType=&quot; + Disaster,&quot;_parent&quot;); 
} 
else{ 
if(&apos;{!Account.RecordType}&apos; == &apos;Field Office&apos;){ 
window.open(&quot;{!$Setup.Org_Level_Settings__c.Content_Object_Prefix__c}/e?CF{!$Setup.Org_Level_Settings__c.Field_Office_Content_Id__c}={!JSENCODE(Account.Name)}&amp;CF{!$Setup.Org_Level_Settings__c.Field_Office_Content_Id__c}_lkid={!Account.Id}&amp;retURL=%2F{!Account.Id}&amp;RecordType=&quot; + Field_Office,&quot;_parent&quot;); 
} 

if(&apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Operational Plan&apos; || &apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Operational Plan Objectives&apos; ||&apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Read Only Operational Plan Objective&apos; || &apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Set Up Plan&apos; || &apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Strategic Plan Support Request&apos; || &apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Suspension - Follow Up&apos; || &apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Suspension - Support Request&apos; || &apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Transition Communication Plan&apos; || &apos;{!Activity_Plan__c.RecordType}&apos; ==&apos;Transition Plan&apos;){ 
window.open(&quot;{!$Setup.Org_Level_Settings__c.Content_Object_Prefix__c}/e?CF{!$Setup.Org_Level_Settings__c.Activity_Plan_Content_Id__c}={!JSENCODE(Activity_Plan__c.Name)}&amp;CF{!$Setup.Org_Level_Settings__c.Activity_Plan_Content_Id__c}_lkid={!Activity_Plan__c.Id}&amp;retURL=%2F{!Activity_Plan__c.Id}&amp;RecordType=&quot; + activityPlan,&quot;_parent&quot;); 

} 
if( &apos;{!Account.RecordType}&apos; ==&apos;Survival Beneficiary&apos; || &apos;{!Account.RecordType}&apos; ==&apos;Sponsorship Beneficiary&apos;){ 
window.open(&quot;{!$Setup.Org_Level_Settings__c.Content_Object_Prefix__c}/e?CF{!$Setup.Org_Level_Settings__c.Ben_Content_Id__c}={!JSENCODE(Account.Name)}&amp;CF{!$Setup.Org_Level_Settings__c.Ben_Content_Id__c}_lkid={!Account.Id}&amp;retURL=%2F{!Account.Id}&amp;RecordType=&quot; + Beneficiary,&quot;_parent&quot;); 

} 

if(&apos;{!Beneficiary_Assessment__c.RecordType}&apos; ==&apos;CDPR&apos; ||&apos;{!Beneficiary_Assessment__c.RecordType}&apos; ==&apos;Development Plan&apos; ||&apos;{!Beneficiary_Assessment__c.RecordType}&apos; ==&apos;Growth Monitoring&apos; ||&apos;{!Beneficiary_Assessment__c.RecordType}&apos; ==&apos;Health Assessment&apos;){ 
window.open(&quot;{!$Setup.Org_Level_Settings__c.Content_Object_Prefix__c}/e?CF{!$Setup.Org_Level_Settings__c.BeneficiaryAssessment_Content_Id__c}={!JSENCODE(Beneficiary_Assessment__c.Name)}&amp;CF{!$Setup.Org_Level_Settings__c.BeneficiaryAssessment_Content_Id__c}_lkid={!Beneficiary_Assessment__c.Id}&amp;retURL=%2F{!Beneficiary_Assessment__c.Id}&amp;RecordType=&quot; + Beneficiary_Assessment,&quot;_parent&quot;); 

} 

if(&apos;{!Beneficiary_Lifecycle_Event__c.RecordType}&apos; ==&apos;Caregiver Death&apos;||&apos;{!Beneficiary_Lifecycle_Event__c.RecordType}&apos; ==&apos;Incident&apos;||&apos;{!Beneficiary_Lifecycle_Event__c.RecordType}&apos; ==&apos;Planned Exit&apos;||&apos;{!Beneficiary_Lifecycle_Event__c.RecordType}&apos; ==&apos;Registration&apos;||&apos;{!Beneficiary_Lifecycle_Event__c.RecordType}&apos; ==&apos;Reinstatement/Return&apos;||&apos;{!Beneficiary_Lifecycle_Event__c.RecordType}&apos; ==&apos;Transfer&apos;||&apos;{!Beneficiary_Lifecycle_Event__c.RecordType}&apos; ==&apos;Transition&apos;||&apos;{!Beneficiary_Lifecycle_Event__c.RecordType}&apos; ==&apos;Unplanned Exit&apos;){ 
window.open(&quot;{!$Setup.Org_Level_Settings__c.Content_Object_Prefix__c}/e?CF{!$Setup.Org_Level_Settings__c.BeneficiaryLifecycleEvent_Content_Id__c}={!JSENCODE(Beneficiary_Lifecycle_Event__c.Name)}&amp;CF{!$Setup.Org_Level_Settings__c.BeneficiaryLifecycleEvent_Content_Id__c}_lkid={!Beneficiary_Lifecycle_Event__c.Id}&amp;retURL=%2F{!Beneficiary_Lifecycle_Event__c.Id}&amp;RecordType=&quot; + Beneficiary_Lifecycle_Event,&quot;_parent&quot;); 

} 

if(&apos;{!Account.RecordType}&apos; ==&apos;ICP&apos;){ 
window.open(&quot;{!$Setup.Org_Level_Settings__c.Content_Object_Prefix__c}/e?CF{!$Setup.Org_Level_Settings__c.ICP_Content_Id__c}={!JSENCODE(Account.Name)}&amp;CF{!$Setup.Org_Level_Settings__c.ICP_Content_Id__c}_lkid={!Account.Id}&amp;retURL=%2F{!Account.Id}&amp;RecordType=&quot; + ICP,&quot;_parent&quot;); 

