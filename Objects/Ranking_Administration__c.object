    <validationRules>
        <fullName>Sum_of_Stage1criteria_must_be_equal_to_1</fullName>
        <active>true</active>
        <description>This validation rule checks if the sum of weights given to stage 1 fields is equal to 100%.</description>
        <errorConditionFormula>IF( ( Statement_Of_Faith_Compatibility__c +Commitment_To_Child_Ministry__c+Location__c) = 1, false, true)</errorConditionFormula>
        <errorMessage>Sum of weights given to all fields of Stage 1 (Statement of faith compatibility, Commitment to child ministry and Location) must be equal to 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sum_of_Stg2_criteria_must_be_equal_to_1</fullName>
        <active>true</active>
        <description>This validation rule checks if the sum of weights given to stage 2 fields is equal to 100%.</description>
        <errorConditionFormula>IF( (Child_Enrollment_Capacity__c+Commitment_To_Recruiting_Christians__c+Commitment_To_Use_Local_Resources__c+Denomination__c+Legal_Registration__c+Physical_Environment__c) = 1, false, true)</errorConditionFormula>
        <errorMessage>Sum of weights given to all fields of Stage 2(Legal Registration, Physical Environment, Child Enrollment Capacity, Commitment to Recruiting Christians, Commitment to Use Local Resources &amp; Denomination) must be equal to 100%</errorMessage>
