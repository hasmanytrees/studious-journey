    <validationRules>
        <fullName>Additional_Funds_Needed_related_Fields</fullName>
        <active>true</active>
        <description>Make Estimated Relief Funds (USD) and Estimated Rehabilitation Funds (USD) required when Additional Funds Needed is True</description>
        <errorConditionFormula>IF( AND (Additional_Funds_Requested__c, OR(ISNULL( Estimated_Rehabilitation_Funds_USD__c ), ISNULL( Estimated_Relief_Funds_USD__c  )) ), true, false)</errorConditionFormula>
        <errorMessage>Please select values for Estimated Relief Funds (USD) and Estimated Rehabilitation Funds (USD).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disaster_Date_should_not_be_later_date</fullName>
        <active>true</active>
        <description>This Validation Rule will ensure that the Disaster Date can be today or a past date</description>
        <errorConditionFormula>Disaster_Date__c  &gt;  TODAY()</errorConditionFormula>
        <errorMessage>Disaster Date can be only be Today&apos;s Date or a Past Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reverse_Additional_Funds_Needed</fullName>
        <active>true</active>
        <description>If a user does not check the Additional Funds Needed box but enters Estimated Relief or Rehabilitation Funds, the system requires the user to check the box</description>
        <errorConditionFormula>IF( OR ( AND ( NOT(ISNULL(Estimated_Rehabilitation_Funds_USD__c)), OR( NOT(Additional_Funds_Requested__c), ISNULL( Estimated_Relief_Funds_USD__c )) ), AND ( NOT(ISNULL(Estimated_Relief_Funds_USD__c)), OR( NOT(Additional_Funds_Requested__c), ISNULL( Estimated_Rehabilitation_Funds_USD__c )) )), true, false)</errorConditionFormula>
        <errorMessage>Please enter values for Estimated Relief Funds (USD) / Estimated Rehabilitation Funds (USD) and check Additional Funds Required if not already.</errorMessage>
    </validationRules>
