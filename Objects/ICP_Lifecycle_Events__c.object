    <validationRules>
        <fullName>Cannot_extend_suspension_at_creation</fullName>
        <active>true</active>
        <description>This validation rule ensure that you cannot mark a suspension as extended while you are creating the suspension record.</description>
        <errorConditionFormula>AND(ISNEW(), RecordType.DeveloperName = &quot;Suspension&quot;, OR(Extension_1__c, Extension_2__c))</errorConditionFormula>
        <errorMessage>You cannot mark a suspension extended before creating it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Error_On_Reactivation_Record</fullName>
        <active>true</active>
        <description>Error on Reactivation record if ICP status is not Suspended.</description>
        <errorConditionFormula>IF( 
$Profile.Name != &apos;Business Admin&apos;, 
AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), ISNEW(),NOT( ISPICKVAL( ICP__r.ICP_Status__c , &quot;Suspended&quot;)), RecordType.DeveloperName = &quot;Reactivation&quot; ), 
IF( 
ISPICKVAL( ICP__r.ICP_Status__c , &quot;Phase Out&quot;), 
AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), ISNEW(), ISPICKVAL( ICP__r.ICP_Status__c , &quot;Phase Out&quot;), RecordType.DeveloperName = &quot;Reactivation&quot;, NOT(ICP__r.Is_Transition_Approved__c == False)), 
AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), ISNEW(),NOT( ISPICKVAL( ICP__r.ICP_Status__c , &quot;Suspended&quot;)), RecordType.DeveloperName = &quot;Reactivation&quot; ) 
) 
)</errorConditionFormula>
        <errorMessage>Reactivation Record can only be created for Suspended ICP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Error_On_Suspension_Record</fullName>
        <active>true</active>
        <description>Error on suspension record if ICP status is not active.</description>
        <errorConditionFormula>AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), ISNEW() , NOT( ISPICKVAL( ICP__r.ICP_Status__c , &quot;Active&quot;) ), RecordType.DeveloperName = &quot;Suspension&quot; )</errorConditionFormula>
        <errorMessage>Suspension Record can only be created for Active ICP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Error_On_Transition_Record</fullName>
        <active>true</active>
        <description>Error on Transition record if ICP status is not Active or Suspended</description>
        <errorConditionFormula>AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), ISNEW() , OR(( ISPICKVAL( ICP__r.ICP_Status__c , &quot;Transitioned&quot;) ),( ISPICKVAL( ICP__r.ICP_Status__c , &quot;Phase Out&quot;) )),RecordType.DeveloperName = &quot;Transition&quot;  )</errorConditionFormula>
        <errorMessage>Transition record can only be created for Active or Suspended ICP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Last_Disbursement_Date_to_ICP_Required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(RecordType.DeveloperName = &quot;Suspension&quot; ,ISBLANK(Last_Disbursement_Date_Of_Funds_From_FO__c),OR(CDSP_Suspension_Of_Funds__c = TRUE, CSP_Suspension_Of_Funds__c = True)),true,False)</errorConditionFormula>
        <errorMessage>Enter Last Disbursement Date to ICP if Sponsorship Funds or Survival / Intervention Funds are being withheld.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Reason_To_Extend</fullName>
        <active>true</active>
        <description>Reason to Extend is mandatory for Number Of Days Of Extension.</description>
        <errorConditionFormula>AND( NOT(ISNULL( Number_Of_Days_Of_Extension__c )) , ISBLANK( Reason_To_Extend_The_Suspension__c ))</errorConditionFormula>
        <errorDisplayField>Reason_To_Extend_The_Suspension__c</errorDisplayField>
        <errorMessage>Reason To Extend The Suspension is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Reason_for_Extension_1</fullName>
        <active>true</active>
        <description>Reason for Extension # 1 is mandatory if Extension # 1 is selected.</description>
        <errorConditionFormula>AND(NOT(ISNEW()), RecordType.DeveloperName = &quot;Suspension&quot;, Extension_1__c  = True, ISBLANK( Reason_for_Extension_1__c ) )</errorConditionFormula>
        <errorDisplayField>Reason_for_Extension_1__c</errorDisplayField>
        <errorMessage>Reason for Extension # 1 is mandatory if Extension # 1 is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Reason_for_Extension_2</fullName>
        <active>true</active>
        <description>Reason for Extension # 2 is mandatory if Extension # 2 is selected.</description>
        <errorConditionFormula>AND(NOT(ISNEW()), RecordType.DeveloperName = &quot;Suspension&quot;, Extension_2__c = True, ISBLANK( Reason_for_Extension_2__c ) )</errorConditionFormula>
        <errorDisplayField>Reason_for_Extension_2__c</errorDisplayField>
        <errorMessage>Reason for Extension # 2 is mandatory if Extension # 2 is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Transition_Date</fullName>
        <active>false</active>
        <description>Transition Date is mandatory if Transition is marked complete.</description>
        <errorConditionFormula>AND( Transition_Complete__c  = true, ISNULL( Actual_Transition_Date__c ))</errorConditionFormula>
        <errorDisplayField>Actual_Transition_Date__c</errorDisplayField>
        <errorMessage>Transition Date is mandatory if Transition is marked completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_User_Editing_Completed_Record</fullName>
        <active>true</active>
        <errorConditionFormula>IF($Profile.Name != &apos;Business Admin&apos;,AND( $Profile.Name != &apos;System Administrator&apos;,!($Label.Disable_For_Data_Load==&apos;true&apos;), ISPICKVAL(PRIORVALUE( Status__c ) , &quot;Completed&quot;), Not(RecordType.DeveloperName = &quot;Suspension&quot;)),AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), ISPICKVAL(PRIORVALUE( Status__c ) , &quot;Completed&quot;), RecordType.DeveloperName = &quot;Reactivation&quot;))</errorConditionFormula>
        <errorMessage>You cannot edit this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_User_Editing_Suspension_Record</fullName>
        <active>true</active>
        <description>This will user from editing suspension record if the end date is populated.</description>
        <errorConditionFormula>AND($Profile.Name!=&apos;System Administrator&apos;,!($Label.Disable_For_Data_Load==&apos;true&apos;), ISPICKVAL(PRIORVALUE( Status__c ) , &quot;Completed&quot;),(RecordType.DeveloperName = &quot;Suspension&quot;),  NOT( ISNULL( Actual_Suspension_End_Date__c ) )  )</errorConditionFormula>
        <errorMessage>You cannot edit this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Suspension_Start_Date_check</fullName>
        <active>false</active>
        <description>Suspension Start Date cannot be a future date</description>
        <errorConditionFormula>AND(Suspension_Start_Date__c  &gt;  TODAY(),RecordType.DeveloperName = &apos;Suspension&apos;)</errorConditionFormula>
        <errorDisplayField>Suspension_Start_Date__c</errorDisplayField>
        <errorMessage>Date cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transition_Complete_If_Status_Is_Approve</fullName>
        <active>true</active>
        <description>Transition can be completed only if status is Approved</description>
        <errorConditionFormula>AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), NOT( ISPICKVAL( Status__c , &quot;Approved&quot;) ), NOT( ISPICKVAL( Status__c , &quot;Completed&quot;) ), Transition_Complete__c  = true)</errorConditionFormula>
        <errorDisplayField>Transition_Complete__c</errorDisplayField>
        <errorMessage>Transition can be completed only after approval from Country Director.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Extension_2</fullName>
        <active>true</active>
        <description>Extension # 2 can only be checked if Extension # 1 is checked.</description>
        <errorConditionFormula>AND(!($Label.Disable_For_Data_Load==&apos;true&apos;),NOT(ISNEW()),RecordType.DeveloperName = &quot;Suspension&quot;, Extension_1__c  = False, Extension_2__c  = True)</errorConditionFormula>
        <errorDisplayField>Extension_2__c</errorDisplayField>
        <errorMessage>Extension # 2 can only be checked if Extension # 1 is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Suspension_Start_And_End_Date</fullName>
        <active>true</active>
        <description>Suspension Start date can not be greater than Suspension End Date</description>
        <errorConditionFormula>AND( NOT( ISNULL( Suspension_Start_Date__c ) ) ,NOT( ISNULL(  Actual_Suspension_End_Date__c ) ),Actual_Suspension_End_Date__c &lt; Suspension_Start_Date__c)</errorConditionFormula>
        <errorMessage>Suspension End Date should be greater than Suspension Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_For_No_Of_Months_For_CDSP</fullName>
        <active>true</active>
        <description>No. Of Months For CDSP for Reactivation cannot be more than 3 months.</description>
        <errorConditionFormula>AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), NOT( ISNULL( No_Of_Months_For_CDSP__c ) ) ,No_Of_Months_For_CDSP__c  &gt; 3)</errorConditionFormula>
        <errorDisplayField>No_Of_Months_For_CDSP__c</errorDisplayField>
        <errorMessage>No. Of Months For Sponsorship cannot be more than 3 months.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_For_No_Of_Months_For_CSP</fullName>
        <active>true</active>
        <description>No. Of Months For CSP/Interventions canot be more than 3 months.</description>
        <errorConditionFormula>AND(!($Label.Disable_For_Data_Load==&apos;true&apos;), NOT( ISNULL(  No_Of_Months_For_CSP_Interventions__c ) ) ,No_Of_Months_For_CSP_Interventions__c &gt; 3)</errorConditionFormula>
        <errorDisplayField>No_Of_Months_For_CSP_Interventions__c</errorDisplayField>
        <errorMessage>No. Of Months For Survival/Interventions cannot be more than 3 months.</errorMessage>
    </validationRules>
</CustomObject>
